{
  "name": "@adventure-x/intelligence-ui",
  "author": "adventureX",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "first-time": "pnpm install && npm run build && next dev",
    "lint": "biome check",
    "build:stubs": "rimraf public/stubs && tsx resources/scripts/build-stubs.ts",
    "prebuild": "pnpm run build:registry && npm run build:stubs",
    "cuc": "node resources/scripts/check-use-client.ts",
    "cdc": "node resources/scripts/check-duplicated-classes.ts",
    "prepare": "husky",
    "build:registry": "rimraf public/registry && tsx resources/scripts/build-registry.ts",
    "dev": "next dev",
    "build": "next build",
    "format": "biome lint --fix && biome check --write",
    "format:unsafe": "biome check --unsafe --write",
    "build:everything": "rimraf public/registry && npm run build:registry && biome format --write __registry__/generated.ts",
    "bb": "pnpm run format && pnpm run build:everything",
    "start": "next start",
    "preview": "pnpm run build && next start",
    "ic": "node resources/scripts/identify-color.ts",
    "typecheck": "tsc --noEmit"
  },
